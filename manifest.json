{
	"manifest_version": 3,
	"name": "Zoom pool party",
	"version": "1.0.0",
	"action": {
		"default_title": "Open",
		"default_popup": "popup.html"
	},
	"background": {
		"service_worker": "scripts/background.js"
	},
	"content_scripts": [
		{
			"js": ["scripts/content.js"],
			"matches": ["https://zoom.us/wc/*", "https://pwa.zoom.us/wc/*"]
		}
	]
}
